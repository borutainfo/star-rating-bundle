{
    "name": "boruta/star-rating-bundle",
    "type": "symfony-bundle",
    "description": "Star Rating Bundle for use with Symfony 6 or 7 using Font Awesome.  This bundle adds the 'rating' form type and rating filter to render ratings. Simple and easy to use. Based on work of 'brokoskokoli' and 'blackknight467'.",
    "keywords": ["rating", "star", "bundle", "symfony", "symfony6", "symfony7"],
    "homepage": "https://github.com/borutainfo/StarRatingBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Boruta",
            "email": "sebastian@boruta.info"
        },
        {
            "name": "Brokoskokoli",
            "email": "brokoskokoli@gmail.com"
        },
        {
            "name": "Derick Felsman",
            "email": "derickfelsman@gmail.com"
        },
        {
            "name": "Steven Ng"
        }
    ],
    "require": {
        "php": ">=8.0",
        "twig/twig": ">=2.4"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=6.0"
    },
    "autoload": {
        "psr-4": { "Boruta\\StarRatingBundle\\": "" }
    },
    "minimum-stability": "dev"
}
